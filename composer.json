{
  "name": "shopgate/cloud-integration-sdk",
  "description": "A PHP framework supporting the implementation of the Shopgate Plugin API and abstracting access to the Shopgate Merchant API.",
  "homepage": "https://github.com/shopgate/cloud-integration-sdk",
  "license": "Apache-2.0",
  "config": {
    "platform": {
      "php": "5.3.3"
    }
  },
  "authors": [
    {
      "name": "Shopgate",
      "email": "interfaces@shopgate.com"
    }
  ],
  "require": {
    "php": ">=5.3",
    "zendframework/zend-json": "2.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*"
  },
  "autoload": {
    "files": [
      "src/router.php",
      "src/request.php",
      "src/response.php",
      "shopgate.php"
    ]
  }
}
